{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", ":configMigration"],
  "labels": ["Renovate"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 10,
  "assignees": ["aazw"],
  "env": {
    "GOPRIVATE": "github.com/aazw/*"
